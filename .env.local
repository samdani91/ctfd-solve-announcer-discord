# Discord Webhook URL for announcing solves
WEBHOOK_URL=your_discord_webhook_url_here

# CTFd instance URL
CTFD_URL=https://your-ctfd-instance.com

# CTFd API Key
CTFD_API_KEY=your_ctfd_api_key_here

# Whether to only announce first bloods (default: true)
ANNOUNCE_FIRST_BLOOD_ONLY=true

# Whether to skip announcing existing solves on startup (default: true)
SKIP_ANNOUNCING_EXISTING_SOLVES=true

# Refresh interval in seconds (default: 5)
REFRESH_INTERVAL_SECONDS=5
